{
  "train": {
    "lr": 4e-5,
    "batch_size": 128,
    "grad_norm": 1.0,
    "warmup": 1000
  },
  "denoise": {
    "in_channels": 3,
    "hid_channels": 64,
    "ch_multipliers": [1, 2, 3, 4],
    "num_res_blocks": 2,
    "apply_attn": [false, true, true, true],
    "embedding_dim": 768,
    "drop_rate": 0.1,
    "head_dim": 64
  },
  "diffusion": {
    "logsnr_min": -20.0,
    "logsnr_max": 20.0,
    "logsnr_schedule": "cosine",
    "model_out_type": "both",
    "model_var_type": "fixed_large",
    "reweight_type": "truncated_snr",
    "loss_type": "mse"
  }
}